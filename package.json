{
	"name": "vue-webpack4-skel",
	"version": "0.0.1",
	"description": "Vue.js Webpack4 skeleton app",
	"index": "index.js",
	"scripts": {
		"build": "webpack-cli --config webpack.prod.js --mode production",
		"dev": "concurrently --kill-others \"npm run backend\" \"npm run frontend\"",
		"format": "prettier --tab-width 4 --write \"src/**/*.vue\" \"src/**/*.js\"",
		"backend": "node backend/index.js",
		"frontend": "webpack-dev-server --config webpack.dev.js --mode development"
	},
	"author": "cdrsh",
	"license": "MIT",
